{
  "name": "Covid",
  "version": "2.2.0",
  "scripts": {
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "android": "ENVFILE=.env.staging react-native run-android --variant=debug --appId=com.joinzoe.covid_zoe.qa",
    "android:devices": " ~/Library/Android/sdk/emulator/emulator -list-avds",
    "ios": "react-native run-ios --scheme 'Covid Debug' --configuration Debug",
    "ios:staging": "ENVFILE=.env.staging npm run ios -- --simulator='iPhone 11'",
    "ios:devices": "xcrun simctl list",
    "web": "expo start --web",
    "test": "jest --maxWorker=2 --runInBand --detectOpenHandles",
    "test:coverage": "jest --maxWorker=2 --coverage --reporters=default --reporters=jest-junit --ci && coveralls < coverage/lcov.info",
    "test:watch": "jest --watch",
    "test:updateSnapshots": "jest -u",
    "test:i18n": "ts-node ./scripts/i18n-check-locale.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:ci": "npm run lint:fix -- --format junit --output-file ~/reports/eslint.xml",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:pre-commit": "eslint --fix",
    "prettier": "prettier --check 'src/**/*.(js|jsx|ts|tsx)'",
    "prettier:fix": "prettier --check 'src/**/*.(js|jsx|ts|tsx)' --write",
    "prettier:pre-commit": "prettier --write",
    "ts-node": "ts-node",
    "mock-server": "ts-node ./mock-server/mockServer.ts --watch",
    "storybook": "start-storybook",
    "clean:git": "git checkout develop && git pull && git fetch --all --prune && for branch in `git branch -vv | grep ': gone]' | awk '{print $1}'`; do git branch -D $branch; done",
    "clear:metro": "rm -rf $TMPDIR/metro-*",
    "reset": "watchman watch-del-all && rm package-lock.json && rm -rf node_modules && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && npm install",
    "postinstall": "cd ios && pod install",
    "fastlane": "ENVFILE=.env bundle exec fastlane",
    "fastlane:ios:register": "bundle exec fastlane register && bundle exec fastlane match adhoc -a com.joinzoe.covid-zoe.qa --force_for_new_devices",
    "fastlane:ios:release": "npm run fastlane ios release",
    "fastlane:android:release": "npm run fastlane android release",
    "bump:patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump",
    "bump:minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump",
    "bump:major": "npm version major --no-git-tag-version && bundle exec fastlane bump"
  },
